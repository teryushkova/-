    "    return smallest_index\n",
    "def sel_sort(arr):\n",
    "    newArr=[]\n",
    "    for i in range(len(arr)):\n",
    "        smallest=find_smallest(arr)\n",
    "        newArr.append(arr.pop(smallest))\n",
    "    return newArr\n",
    "array = sel_sort([x]) \n",
    "print (array)"
