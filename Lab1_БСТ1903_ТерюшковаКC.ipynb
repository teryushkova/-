   "source": [
    "def heap_sort_helper(A, n, i):\n",
    "    l=left(i)\n",
    "    r=right(i)\n",
    "    if l<n and A[l]>A[i]:\n",
    "        largest = 1\n",
    "    else:\n",
    "        largest = i\n",
    "    if r<n and A[r]> A[largest]:\n",
    "        largest=r\n",
    "    if largest !=i:\n",
    "        A[i], A[largest]= A[largest], A[i]\n",
    "        heap_sort_helper(A, n, largest)\n",
    "def left(i):\n",
    "    return 2*i+1\n",
    "def right(i):\n",
    "    return 2*i+2\n",
    "def heap_sort(A):\n",
    "    n=len(A)\n",
    "    for i in range(n,-1,-1):\n",
    "        heap_sort_helper(A,n,i)\n",
    "    for i in range(n-1, 0, -1):\n",
    "        heap_sort_helper(A, i, 0)\n",
    "a=[x]\n",
    "heap_sort(a)\n",
    "print(a)"
