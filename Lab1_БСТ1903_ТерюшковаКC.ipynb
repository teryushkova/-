    "def shell_sort(array):\n",
    "    n= len(array)\n",
    "    step=n//2\n",
    "    while step>0:\n",
    "        for i in range (step, n):\n",
    "            j=i\n",
    "            while j>= step and array[j]<array[j-step]:\n",
    "                array[j], array[j-step] =array[j-step],array[j]\n",
    "                j=j-step\n",
    "                step= step//2\n",
    "                \n",
    "arr=shell_sort(x)\n",
    "print(arr)"
   ]
